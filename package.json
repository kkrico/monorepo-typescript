{
  "name": "monorepo-typescript",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "concurrently --kill-others \"yarn start-api\" \"yarn start-web\"",
    "start-api": "yarn workspace @monorepo-typescript/api start",
